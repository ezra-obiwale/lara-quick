{
    "name": "d-scribe/laraquick",
    "type": "library",
    "description": "A collection of classes to be extended/used in laravel applications for quick development",
    "license": "MIT",
    "keywords": [
        "laravel",
        "quick",
        "api",
        "web",
        "controller",
        "traits"
    ],
    "authors": [
        {
            "name": "Ezra Obiwale",
            "email": "contact@ezraobiwale.com",
            "homepage": "http://ezraobiwale.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "~6.0",
        "maatwebsite/excel": "~3.0",
        "predis/predis": "^1.1",
        "spatie/laravel-query-builder": "^2.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraquick\\Providers\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Laraquick\\": "src/"
        }
    },
    "require-dev": {
        "mpociot/laravel-apidoc-generator": "^3.2"
    }
}
